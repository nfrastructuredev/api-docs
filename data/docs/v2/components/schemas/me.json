{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "description": "Logged in users information",
  "properties": {
    "_embedded": {
      "type": "object",
      "properties": {
        "recent": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "label": {
                "type": "string",
                "example": "Project: Zones installation"
              },
              "_links": {
                "type": "object",
                "readOnly": true,
                "properties": {
                  "self": {
                    "type": "string",
                    "format": "url",
                    "readOnly": true,
                    "example": "https://api.nterprise.com/projects/eb610ff8-2a2e-47e9-be14-8676d2573701"
                  }
                }
              }
            }
          }
        },
        "launcher": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "label": {
                "type": "string",
                "example": "Project: Zones installation"
              },
              "_links": {
                "type": "object",
                "readOnly": true,
                "properties": {
                  "self": {
                    "type": "string",
                    "format": "url",
                    "readOnly": true,
                    "example": "https://api.nterprise.com/projects/eb610ff8-2a2e-47e9-be14-8676d2573701"
                  }
                }
              }
            }
          }
        },
        "favorites": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "label": {
                "type": "string",
                "example": "Customer: Zones"
              },
              "_links": {
                "type": "object",
                "readOnly": true,
                "properties": {
                  "self": {
                    "type": "string",
                    "format": "url",
                    "readOnly": true,
                    "example": "https://api.nterprise.com/customers/eb610ff8-2a2e-47e9-be14-8676d2573701"
                  }
                }
              }
            }
          }
        }
      }
    },
    "_links": {
      "type": "object",
      "readOnly": true,
      "properties": {
        "nter:dashboard": {
          "type": "object",
          "readOnly": true,
          "properties": {
            "href": {
              "type": "string",
              "format": "url",
              "example": "https://api.nterprise.com/dashboard"
            }
          }
        },
        "nter:customers": {
          "type": "object",
          "readOnly": true,
          "properties": {
            "href": {
              "type": "string",
              "format": "url",
              "example": "https://api.nterprise.com/customers/{customer_id}"
            },
            "templated": {
              "type": "boolean",
              "description": "Weather the link is templated"
            }
          }
        },
        "nter:extentions": {
          "type": "object",
          "readOnly": true,
          "properties": {
            "href": {
              "type": "string",
              "format": "url",
              "example": "https://api.nterprise.com/extensions"
            }
          }
        },
        "nter:settings": {
          "type": "object",
          "readOnly": true,
          "properties": {
            "href": {
              "type": "string",
              "format": "url",
              "example": "https://api.nterprise.com/settings"
            }
          }
        },
        "nter:techs": {
          "type": "object",
          "readOnly": true,
          "properties": {
            "href": {
              "type": "string",
              "format": "url",
              "example": "https://api.nterprise.com/techs"
            }
          }
        },
        "nter:logout": {
          "type": "object",
          "readOnly": true,
          "properties": {
            "href": {
              "type": "string",
              "format": "url",
              "example": "https://api.nterprise.com/logout"
            }
          }
        }
      },
      "allOf": [
        {
          "$ref": "/v2/components/schemas/linkSelf.json"
        },
        {
          "$ref": "/v2/components/schemas/linkNterCurie.json"
        }
      ]
    }
  },
  "allOf": [
    {
      "$ref": "/v2/components/schemas/user.json"
    }
  ]
}
